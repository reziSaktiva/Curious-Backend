           actions={\r\n                !loading && [\r\n                  <Row gutter={[48, 0]}>\r\n                        <Col xs={8} sm={8} md={8} lg={8} xl={8}>\r\n                  <LikeButton likeCount={ getPost.likeCount? getPost.likeCount: 0 } likes={getPost.likes} id={ getPost.id } />\r\n                        </Col>\r\n                        <Col xs={8} sm={8} md={8} lg={8} xl={8}>\r\n                        <CommentButton commentCount={ getPost.commentCount? getPost.commentCount : 0 } />\r\n                        </Col>\r\n                        <Col xs={8} sm={8} md={8} lg={8} xl={8}>\r\n                        <RepostButton />\r\n                        </Col>\r\n                      </Row>\r\n                ]\r\n              }\r\n              \r\n            >\r\n              <Skeleton loading={loading} active avatar>\r\n                <List.Item.Meta\r\n                  title={<div>\r\n                    <Row>\r\n                      <Col span={12}>\r\n                      <img src={Pin} style={{width: 20, position: \"center\"}}/>{address}\r\n                      </Col>\r\n                      <Col span={12} style={{textAlign: \"right\"}}>\r\n                        <Dropdown overlay={\r\n                          <Menu>\r\n                          <Menu.Item key=\"0\">\r\n                            Subscribe\r\n                          </Menu.Item>\r\n                          <Menu.Item key=\"1\" onClick={e=> console.log(e)}>\r\n                            Mute\r\n                          </Menu.Item>\r\n                          <Menu.Item key=\"3\">\r\n                            Report\r\n                          </Menu.Item>\r\n                        </Menu>\r\n                        } trigger={['click']} placement=\"bottomRight\">\r\n                        <a className=\"ant-dropdown-link\" onClick={e => e.preventDefault()}>\r\n                          <EllipsisOutlined />\r\n                        </a>\r\n                      </Dropdown>\r\n                      </Col>\r\n                  </Row>\r\n                    </div>}\r\n                  description={moment(getPost.createdAt).fromNow()}\r\n                />\r\n                {getPost.text}\r\n              </Skeleton>\r\n            </List.Item>\r\n            {comments.length == 0 ? null : (\r\n              <List\r\n              itemLayout=\"vertical\"\r\n              size=\"large\"\r\n              dataSource={comments}\r\n              renderItem={\r\n                item => (\r\n                <Card style={{ width: 300, marginTop: 16 }} loading={loading}>\r\n              <Meta\r\n                avatar={\r\n                  <Link to=\"/profile\">\r\n                  <Avatar size={50}  style={{ backgroundColor:item.colorCode, backgroundImage: `url(${item.photoProfile})`, backgroundSize: 50}} />\r\n                  </Link>\r\n                }\r\n                title={item.displayName}\r\n                description={moment(item.createdAt).fromNow()}\r\n              />\r\n              {item.text}\r\n            </Card>)\r\n            }\r\n            />\r\n            ) }\r\n            \r\n        <Search\r\n      placeholder=\"Input comment here...\"\r\n      allowClear\r\n      enterButton=\"Post\"\r\n      size=\"large\"\r\n    />\r\n          </List>\r\n        )\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            {postMarkUp}\r\n        </div>\r\n    )\r\n}\r\n","C:\\Web Dev\\project-curious\\frontend\\src\\context\\Context.jsx",[],"C:\\Web Dev\\project-curious\\frontend\\src\\pages\\map.jsx",[],"C:\\Web Dev\\project-curious\\frontend\\src\\pages\\landingPage.jsx",["173"],"import React from 'react'\r\nimport '../App.css'\r\nimport { Col, Row, Button } from 'antd'\r\nimport { Grid } from 'semantic-ui-react'\r\n\r\nimport LoginFacebook from '../components/LoginFacebookButton'\r\nimport { Link } from 'react-router-dom'\r\n\r\nfunction LandingPage(props) {\r\n    return (\r\n        <div>\r